{
    "name": "bruneiExhibit/sitemaps",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "BruneiExhibit\\Sitemaps\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BruneiExhibit\\Sitemaps\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
